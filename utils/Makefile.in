# Network UPS Tools: common

CC	= @CC@
CFLAGS	= -I../include @CFLAGS@

PROGS = upsconfig

UPSCONFIGOBJ = ../common/data_types.o ../common/tree.o \
../common/stack.o ../lib/libupsconfig.o ../common/nutparser.o upsconfig.o
LIBOBJ	= @LIBOBJ@

all: $(PROGS)

upsconfig: $(UPSCONFIGOBJ) $(LIBOBJ)
	$(CC) $(CFLAGS) -o upsconfig $(UPSCONFIGOBJ) $(LIBOBJ)

upsconfig.o : upsconfig.c
	$(CC) $(CFLAGS) -c upsconfig.c
	
clean:
	rm -f *~ *.o

distclean: clean

install: all
